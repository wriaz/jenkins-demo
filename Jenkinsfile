pipeline {
    agent any

    stages {        
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Here'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Here'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Here'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing Here'
            }
        }
    }
}
